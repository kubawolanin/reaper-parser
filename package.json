{
  "name": "reaper-parser",
  "version": "0.0.1",
  "description": "Parse *.rpp files",
  "main": "index.js",
  "scripts": {
    "start": "node --harmony index",
    "clean": "rimraf posts/*",
    "test": "yarn lint && tape test/*-spec.js | tap-dot",
    "coverage": "nyc --reporter=lcov --reporter=text-summary tape test/*-spec.js nolarge",
    "lint": "eslint .",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "grammar": "nearleyc",
    "nearleyc": "nearleyc"
  },
  "keywords": [
    "reaper",
    "daw",
    "parser"
  ],
  "author": "Kuba Wolanin <hi@kubawolanin.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "mkdirp": "^1.0.4",
    "nyc": "^15.0.1",
    "rimraf": "^3.0.2",
    "tap-dot": "^2.0.0",
    "tape": "^5.0.0"
  },
  "dependencies": {
    "config": "^3.3.1",
    "midi-writer-js": "^1.7.4",
    "minimist": "^1.2.5",
    "nearley": "^2.19.3"
  }
}